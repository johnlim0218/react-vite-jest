{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-alert": "off",
    "no-console": "warn",
    "no-restricted-globals": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "no-script-url": "off",
    "no-useless-escape": "off",
    "react-hooks/exhaustive-deps": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true,
    "node": true
  }
}
